{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "graham-campbell/github": "^12.6",
        "guzzlehttp/guzzle": "^7.9.2",
        "laravel/framework": "^11.41.3",
        "laravel/jetstream": "^5.3.4",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.10.1",
        "laravel/unfenced": "dev-main",
        "livewire/livewire": "^3.0",
        "spatie/laravel-sitemap": "^7.3.4",
        "symfony/browser-kit": "^7.2",
        "torchlight/torchlight-commonmark": "dev-main",
        "torchlight/torchlight-laravel": "^0.6"
    },
    "require-dev": {
        "fakerphp/faker": "^1.24.1",
        "laravel/pint": "^1.20",
        "mockery/mockery": "^1.7",
        "nunomaduro/collision": "^8.5",
        "phpunit/phpunit": "^11"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:laravel-labs/unfenced.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/caendesilva/torchlight-commonmark-php"
        }
    ],
    "autoload": {
        "files": [
            "bootstrap/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
